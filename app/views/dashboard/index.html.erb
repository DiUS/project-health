<div class="dashboard">
  <h1><%= @project.name %></h1>

  <div class="indicators">
    <% @indicator_measures.each do | measure | %>
      <div class='indicator'>
        <span class="title"><%= measure.name %></span>
        <span class="mean"><%= '%.2f' % measure.mean %></span>
      </div>
    <% end %>
  </div>

</div>

